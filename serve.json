{
  "public": ".",
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "**/*.mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/javascript"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/assets/js/analytics.js",
      "destination": "/assets/js/analytics.js"
    }
  ]
}
