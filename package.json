{
  "name": "danielwebsite",
  "version": "1.0.0",
  "type": "module",
  "description": "My personal portfolio website showcasing my projects and skills.",
  "main": "./assets/index.js",
  "scripts": {
    "start": "node server.js",
    "ngrok": "ngrok http --url=tender-randomly-skunk.ngrok-free.app 3000",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress open",
    "build": "webpack --mode production",
    "css": "postcss assets/css/style.css -o assets/css/style.min.css --verbose",
    "css:watch": "postcss assets/css/style.css -o assets/css/style.min.css --watch"
  },
  "repository": {
    "type": "git",
