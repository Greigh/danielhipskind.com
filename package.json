{
  "name": "danielwebsite",
  "version": "1.0.0",
  "type": "module",
  "description": "My personal portfolio website showcasing my projects and skills.",
  "main": "./assets/js/index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "DEBUG=app:* nodemon server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"{assets,analytics,server}/**/*.{js,css,html}\"",
    "css": "postcss assets/css/style.css -o assets/css/style.min.css",
    "css:analytics": "postcss analytics/css/dashboard.css -o analytics/css/dashboard.min.css",
    "optimize-images": "node scripts/optimizeImages.js",
    "test": "mocha server/**/*.test.js analytics/**/*.test.js",
    "test:watch": "mocha server/tests/**/*.test.js --watch --require esm",
    "build": "npm run css && npm run css:analytics && npm run optimize-images",
    "dev:analytics": "DEBUG=analytics:* nodemon server.js",
    "test:content": "mocha server/tests/contentManager.test.js --require esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greigh/DanielNewWebsite.git"
  },
  "author": "Daniel Hipskind aka Greigh",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/Greigh/DanielNewWebsite/issues"
  },
  "homepage": "https://github.com/Greigh/DanielNewWebsite#readme",
  "dependencies": {
    "@kurkle/color": "^0.3.4",
    "@octokit/core": "^6.1.4",
    "axios": "^1.8.2",
    "bcryptjs": "^3.0.2",
    "bottleneck": "^2.19.5",
    "chart.js": "^4.4.8",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-ws": "^5.0.2",
    "feed": "^4.2.2",
    "fs-extra": "^11.3.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "playwright": "^1.51.0",
    "rss": "^1.2.2",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.14.0",
    "sharp": "^0.33.5",
    "winston": "^3.17.0",
    "ws": "^8.18.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "esm": "^3.2.25",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.2.5",
    "sinon": "^19.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
