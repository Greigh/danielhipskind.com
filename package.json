    "url": "https://github.com/Greigh/DanielNewWebsite/issues"
  },
  "homepage": "https://github.com/Greigh/DanielNewWebsite#readme",
  "dependencies": {
    "@octokit/core": "^6.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "cypress": "^13.6.6",
