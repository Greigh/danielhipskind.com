    "nodemon": "^3.0.3",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.5",
    "typescript": "^5.3.3",
    "webpack": "^5.98.0",
